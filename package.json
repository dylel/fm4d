{
  "name": "with-svelte",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "check-types": "turbo run check-types",
    "clean": "git clean -xfd -e .env -e .env.*",
    "clean:workspaces": "pnpm -r exec -- rimraf node_modules .svelte-kit dist .turbo && rimraf node_modules .turbo"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.6.0",
    "prettier-plugin-svelte": "^3.4.0",
    "rimraf": "^6.1.2",
    "turbo": "^2.6.1"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=20"
  }
}
